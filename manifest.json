{
  "manifest_version": 2,

  "name": "OnCall Alert",
  "description" : "The main purpose of this extension is to notify users within the workplace",
  "version": "1",
  "email" : "omarsmak@gmail.com",
  "browser_action": {
    "default_popup": "popup.html",
     "default_icon": "icons/icon_128.png"
  },
  "permissions": [
    "https://xxxxx.jira.com/",
    "storage",
    "notifications"
  ],
    "background": {
        "page":  "background.html"
    },

    "icons": { "16": "icons/icon_16.png",
        "48": "icons/icon_48.png",
        "128": "icons/icon_128.png" },

    "web_accessible_resources": [
        "icons/icon_128.png"
    ]
}
